<web-app>
  


   
    <servlet>
        <servlet-name>csvSearch</servlet-name>
        <servlet-class>servlet.csvjdbc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>csvSearch</servlet-name>
        <url-pattern>/search.do</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>excelSearch</servlet-name>
        <servlet-class>servlet.readExcel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>excelSearch</servlet-name>
        <url-pattern>/search1.do</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>dbMovie</servlet-name>
        <servlet-class>servlet.readDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dbMovie</servlet-name>
        <url-pattern>/movie.do</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>dbBook</servlet-name>
        <servlet-class>servlet.bookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dbBook</servlet-name>
        <url-pattern>/book.do</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>    
</web-app>
